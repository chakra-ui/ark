{
  "Basic": "import { Dialog } from '@ark-ui/solid'\nimport { Portal } from 'solid-js/web'\n\nconst Basic = () => {\n  return (\n    <Dialog.Root>\n      <Dialog.Trigger>Open Dialog</Dialog.Trigger>\n      <Portal>\n        <Dialog.Backdrop />\n        <Dialog.Container>\n          <Dialog.Content>\n            <Dialog.Title>Dialog Title</Dialog.Title>\n            <Dialog.Description>Dialog Description</Dialog.Description>\n            <div>\n              <input placeholder=\"Enter name...\" />\n              <button>Save</button>\n            </div>\n            <Dialog.CloseTrigger>Close</Dialog.CloseTrigger>\n          </Dialog.Content>\n        </Dialog.Container>\n      </Portal>\n    </Dialog.Root>\n  )\n}\n",
  "DialogWithRenderFn": "import { Dialog } from '@ark-ui/solid'\nimport { Portal } from 'solid-js/web'\n\nconst DialogWithRenderFn = () => {\n  return (\n    <Dialog.Root>\n      {(api) => (\n        <>\n          <Dialog.Trigger>Open Dialog</Dialog.Trigger>\n          <Portal>\n            <Dialog.Backdrop />\n            <Dialog.Container>\n              <Dialog.Content>\n                <Dialog.Title>Dialog Title</Dialog.Title>\n                <Dialog.Description>Dialog Description</Dialog.Description>\n                <Dialog.CloseTrigger>Close</Dialog.CloseTrigger>\n              </Dialog.Content>\n            </Dialog.Container>\n          </Portal>\n          <p>Dialog is {api().isOpen ? 'open' : 'closed'}</p>\n        </>\n      )}\n    </Dialog.Root>\n  )\n}\n"
}
