  "Basic": "<script setup lang=\"ts\">\nimport { ref } from \"vue\";\nimport { Presence } from \"@ark-ui/vue\";\n\nconst isPresent = ref(false);\n</script>\n\n<template>\n  <div>\n    <button @click=\"isPresent = !isPresent\">Toggle</button>\n    <Presence :present=\"isPresent\">Hidden and Hidden </Presence>\n  </div>\n</template>\n",
