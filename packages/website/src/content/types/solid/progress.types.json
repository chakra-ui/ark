{
  "Root": {
    "as": { "type": "ValidComponent", "isRequired": false },
    "dir": {
      "type": "'ltr' | 'rtl'",
      "defaultValue": "\"ltr\"",
      "isRequired": false,
      "description": "The document's text/writing direction."
    },
    "getRootNode": {
      "type": "() => Node | ShadowRoot | Document",
      "isRequired": false,
      "description": "A root node to correctly resolve document in custom environments. E.x.: Iframes, Electron."
    },
    "id": {
      "type": "string",
      "isRequired": false,
      "description": "The unique identifier of the machine."
    },
    "max": {
      "type": "number",
      "isRequired": false,
      "description": "The maximum allowed value of the progress bar."
    },
    "min": {
      "type": "number",
      "isRequired": false,
      "description": "The minimum allowed value of the progress bar."
    },
    "orientation": {
      "type": "Orientation",
      "defaultValue": "\"horizontal\"",
      "isRequired": false,
      "description": "The orientation of the element."
    },
    "translations": {
      "type": "IntlTranslations",
      "isRequired": false,
      "description": "The localized messages to use."
    },
    "value": {
      "type": "number",
      "isRequired": false,
      "description": "The current value of the progress bar."
    }
  },
  "View": {
    "state": { "type": "ProgressState", "isRequired": true },
    "as": { "type": "ValidComponent", "isRequired": false }
  },
  "Label": { "as": { "type": "ValidComponent", "isRequired": false } },
  "Range": { "as": { "type": "ValidComponent", "isRequired": false } },
  "Track": { "as": { "type": "ValidComponent", "isRequired": false } },
  "Circle": { "as": { "type": "ValidComponent", "isRequired": false } },
  "ValueText": { "as": { "type": "ValidComponent", "isRequired": false } },
  "CircleRange": { "as": { "type": "ValidComponent", "isRequired": false } },
  "CircleTrack": { "as": { "type": "ValidComponent", "isRequired": false } }
}
