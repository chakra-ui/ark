{
  "Basic": "const Basic = () => {\n  return (\n    <Dialog>\n      <DialogTrigger>click me</DialogTrigger>\n      <Portal>\n        <DialogBackdrop />\n        <DialogContainer>\n          <DialogContent>\n            <DialogTitle>Dialog Title</DialogTitle>\n            <DialogDescription>Dialog Description</DialogDescription>\n            <div>\n              <input placeholder=\"Enter name...\" />\n              <button>Save</button>\n            </div>\n            <DialogCloseTrigger>close</DialogCloseTrigger>\n          </DialogContent>\n        </DialogContainer>\n      </Portal>\n    </Dialog>\n  )\n}",
  "Controlled": "const Controlled = () => {\n  const [isOpen, setIsOpen] = useState(false)\n  return (\n    <>\n      <button onClick={() => setIsOpen(!isOpen)}>Toggle</button>\n      <Dialog open={isOpen} onClose={() => setIsOpen(false)}>\n        <Portal>\n          <DialogBackdrop />\n          <DialogContainer>\n            <DialogContent>\n              <DialogTitle>Dialog Title</DialogTitle>\n              <DialogDescription>Dialog Description</DialogDescription>\n              <div>\n                <input placeholder=\"Enter name...\" />\n                <button>Save</button>\n              </div>\n              <DialogCloseTrigger>close</DialogCloseTrigger>\n            </DialogContent>\n          </DialogContainer>\n        </Portal>\n      </Dialog>\n    </>\n  )\n}",
  "DialogWithRenderFn": "const DialogWithRenderFn = () => {\n  return (\n    <Dialog>\n      {({ isOpen }) => (\n        <>\n          <DialogTrigger>Open Dialog</DialogTrigger>\n          <Portal>\n            <DialogBackdrop />\n            <DialogContainer>\n              <DialogContent>\n                <DialogTitle>Dialog Title</DialogTitle>\n                <DialogDescription>Dialog Description</DialogDescription>\n                <DialogCloseTrigger>Close</DialogCloseTrigger>\n              </DialogContent>\n            </DialogContainer>\n          </Portal>\n          <p>Dialog is {isOpen ? 'open' : 'closed'}</p>\n        </>\n      )}\n    </Dialog>\n  )\n}"
}
