{
  "Basic": "const Basic = () => (\n  <Popover>\n    <PopoverTrigger asChild>\n      <button>click me</button>\n    </PopoverTrigger>\n    <PopoverPositioner>\n      <PopoverContent>\n        <PopoverArrow>\n          <PopoverArrowTip />\n        </PopoverArrow>\n        <PopoverTitle>Title</PopoverTitle>\n        <PopoverDescription>Description</PopoverDescription>\n        <input type=\"text\" />\n        <PopoverCloseTrigger>\n          <button>close</button>\n        </PopoverCloseTrigger>\n      </PopoverContent>\n    </PopoverPositioner>\n  </Popover>\n)",
  "Controlled": "const Controlled = () => {\n  const [isOpen, setIsOpen] = createSignal(false)\n  return (\n    <>\n      <button onClick={() => setIsOpen((prev) => !prev)}>click me</button>\n      <Popover open={isOpen()}>\n        <PopoverAnchor>\n          <span>anchor</span>\n        </PopoverAnchor>\n        <PopoverPositioner>\n          <PopoverContent>\n            <PopoverArrow>\n              <PopoverArrowTip />\n            </PopoverArrow>\n            <PopoverTitle>Title</PopoverTitle>\n            <PopoverDescription>Description</PopoverDescription>\n            <input type=\"text\" />\n            <PopoverCloseTrigger>\n              <button>close</button>\n            </PopoverCloseTrigger>\n          </PopoverContent>\n        </PopoverPositioner>\n      </Popover>\n    </>\n  )\n}"
}
