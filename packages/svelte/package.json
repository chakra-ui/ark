{
  "name": "@ark-ui/svelte",
  "type": "module",
  "version": "5.0.2",
  "description": "A collection of unstyled, accessible UI components for Svelte",
  "keywords": [
    "accordion",
    "angle slider",
    "avatar",
    "carousel",
    "checkbox",
    "clipboard",
    "collapsible",
    "color picker",
    "combobox",
    "date picker",
    "dialog",
    "editable",
    "field",
    "fieldset",
    "file upload",
    "frame",
    "hover card",
    "menu",
    "number input",
    "pagination",
    "pin input",
    "popover",
    "portal",
    "progress",
    "radio group",
    "rating group",
    "segment group",
    "select",
    "signature pad",
    "slider",
    "splitter",
    "switch",
    "tabs",
    "tags input",
    "time picker",
    "timer",
    "toast",
    "toggle group",
    "tooltip",
    "tree view",
    "password input"
  ],
  "license": "MIT",
  "homepage": "https://ark-ui.com",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chakra-ui/ark.git",
    "directory": "packages/svelte"
  },
  "bugs": {
    "url": "https://github.com/chakra-ui/ark/issues"
  },
  "svelte": "./src/lib/index.ts",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "!dist/**/*.stories.*",
    "!dist/**/examples/**",
    "!dist/**/*.test.*"
  ],
  "exports": {
    ".": {
      "types": "./src/lib/index.ts",
      "svelte": "./src/lib/index.ts"
    },
    "./anatomy": {
      "types": "./src/lib/components/anatomy.ts",
      "svelte": "./src/lib/components/anatomy.ts"
    },
    "./environment": {
      "types": "./src/lib/providers/environment/index.ts",
      "svelte": "./src/lib/providers/environment/index.ts"
    },
    "./locale": {
      "types": "./src/lib/providers/locale/index.ts",
      "svelte": "./src/lib/providers/locale/index.ts"
    },
    "./*": {
      "types": "./src/lib/components/*/index.ts",
      "svelte": "./src/lib/components/*/index.ts"
    }
  },
  "scripts": {
    "build": "svelte-kit sync && svelte-package --tsconfig ./tsconfig.build.json",
    "start": "svelte-kit sync && vite dev",
    "dev": "storybook dev -p 6006",
    "storybook": "storybook dev -p 6006",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ci": "vitest --run",
    "typecheck": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "prepack": "clean-package",
    "postpack": "clean-package restore",
    "release-it": "release-it --config ../../release-it.json"
  },
  "clean-package": {
    "replace": {
      "svelte": "./dist/index.js",
      "exports.\\..svelte": "./dist/index.js",
      "exports.\\..types": "./dist/index.d.ts",
      "exports.\\./anatomy.svelte": "./dist/components/anatomy.js",
      "exports.\\./anatomy.types": "./dist/components/anatomy.d.ts",
      "exports.\\./locale.svelte": "./dist/providers/locale/index.js",
      "exports.\\./locale.types": "./dist/providers/locale/index.d.ts",
      "exports.\\./environment.svelte": "./dist/providers/environment/index.js",
      "exports.\\./environment.types": "./dist/providers/environment/index.d.ts",
      "exports.\\./*.svelte": "./dist/components/*/index.js",
      "exports.\\./*.types": "./dist/components/*/index.d.ts"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "dependencies": {
    "@internationalized/date": "3.8.2",
    "@zag-js/accordion": "1.17.2",
    "@zag-js/angle-slider": "1.17.2",
    "@zag-js/anatomy": "1.17.2",
    "@zag-js/auto-resize": "1.17.2",
    "@zag-js/avatar": "1.17.2",
    "@zag-js/carousel": "1.17.2",
    "@zag-js/checkbox": "1.17.2",
    "@zag-js/clipboard": "1.17.2",
    "@zag-js/collapsible": "1.17.2",
    "@zag-js/collection": "1.17.2",
    "@zag-js/color-picker": "1.17.2",
    "@zag-js/color-utils": "1.17.2",
    "@zag-js/combobox": "1.17.2",
    "@zag-js/core": "1.17.2",
    "@zag-js/date-picker": "1.17.2",
    "@zag-js/date-utils": "1.17.2",
    "@zag-js/dialog": "1.17.2",
    "@zag-js/dom-query": "1.17.2",
    "@zag-js/editable": "1.17.2",
    "@zag-js/file-upload": "1.17.2",
    "@zag-js/file-utils": "1.17.2",
    "@zag-js/focus-trap": "1.17.2",
    "@zag-js/floating-panel": "1.17.2",
    "@zag-js/highlight-word": "1.17.2",
    "@zag-js/hover-card": "1.17.2",
    "@zag-js/i18n-utils": "1.17.2",
    "@zag-js/listbox": "1.17.2",
    "@zag-js/menu": "1.17.2",
    "@zag-js/number-input": "1.17.2",
    "@zag-js/pagination": "1.17.2",
    "@zag-js/password-input": "1.17.2",
    "@zag-js/pin-input": "1.17.2",
    "@zag-js/popover": "1.17.2",
    "@zag-js/presence": "1.17.2",
    "@zag-js/progress": "1.17.2",
    "@zag-js/qr-code": "1.17.2",
    "@zag-js/radio-group": "1.17.2",
    "@zag-js/rating-group": "1.17.2",
    "@zag-js/svelte": "1.17.2",
    "@zag-js/select": "1.17.2",
    "@zag-js/signature-pad": "1.17.2",
    "@zag-js/slider": "1.17.2",
    "@zag-js/splitter": "1.17.2",
    "@zag-js/steps": "1.17.2",
    "@zag-js/switch": "1.17.2",
    "@zag-js/tabs": "1.17.2",
    "@zag-js/tags-input": "1.17.2",
    "@zag-js/time-picker": "1.17.2",
    "@zag-js/timer": "1.17.2",
    "@zag-js/toast": "1.17.2",
    "@zag-js/toggle": "1.17.2",
    "@zag-js/toggle-group": "1.17.2",
    "@zag-js/tooltip": "1.17.2",
    "@zag-js/tour": "1.17.2",
    "@zag-js/tree-view": "1.17.2",
    "@zag-js/types": "1.17.2",
    "@zag-js/utils": "1.17.2"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "9.0.10",
    "@storybook/sveltekit": "9.0.10",
    "@sveltejs/adapter-auto": "6.0.1",
    "@sveltejs/kit": "2.21.5",
    "@sveltejs/package": "2.3.11",
    "@sveltejs/vite-plugin-svelte": "5.1.0",
    "@tanstack/svelte-form": "1.12.3",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/svelte": "5.2.8",
    "@testing-library/user-event": "14.6.1",
    "@vitest/coverage-v8": "3.2.3",
    "clean-package": "2.2.0",
    "lucide-svelte": "0.515.0",
    "storybook": "9.0.10",
    "svelte": "5.34.3",
    "svelte-check": "4.2.1",
    "tslib": "2.8.1",
    "typescript": "5.8.3",
    "vite": "6.3.5",
    "vitest": "3.2.3"
  },
  "peerDependencies": {
    "svelte": ">=5.20.0"
  }
}
