{
  "Provider": {
    "defaultLocale": {
      "type": "string",
      "defaultValue": "'en-US'",
      "isRequired": false,
      "description": "The default locale to use if no locale is provided via props."
    },
    "getRootNode": { "type": "() => Node | ShadowRoot | Document", "isRequired": false },
    "locale": { "type": "string", "isRequired": false },
    "onLocaleChange": { "type": "(locale: Locale) => void", "isRequired": false }
  }
}
