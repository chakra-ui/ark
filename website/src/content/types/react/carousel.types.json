{
  "Control": {
    "props": {
      "asChild": {
        "type": "boolean",
        "isRequired": false,
        "description": "Render as a different element type."
      }
    },
    "element": "HTMLDivElement"
  },
  "IndicatorGroup": {
    "props": {
      "asChild": {
        "type": "boolean",
        "isRequired": false,
        "description": "Render as a different element type."
      }
    },
    "element": "HTMLDivElement"
  },
  "Indicator": {
    "props": {
      "index": { "type": "number", "isRequired": true },
      "asChild": {
        "type": "boolean",
        "isRequired": false,
        "description": "Render as a different element type."
      },
      "readOnly": { "type": "boolean", "isRequired": false }
    },
    "element": "HTMLButtonElement"
  },
  "ItemGroup": {
    "props": {
      "asChild": {
        "type": "boolean",
        "isRequired": false,
        "description": "Render as a different element type."
      }
    },
    "element": "HTMLDivElement"
  },
  "Item": {
    "props": {
      "index": { "type": "number", "isRequired": true, "description": "The index of the item." },
      "asChild": {
        "type": "boolean",
        "isRequired": false,
        "description": "Render as a different element type."
      }
    },
    "element": "HTMLDivElement"
  },
  "NextTrigger": {
    "props": {
      "asChild": {
        "type": "boolean",
        "isRequired": false,
        "description": "Render as a different element type."
      }
    },
    "element": "HTMLButtonElement"
  },
  "PrevTrigger": {
    "props": {
      "asChild": {
        "type": "boolean",
        "isRequired": false,
        "description": "Render as a different element type."
      }
    },
    "element": "HTMLButtonElement"
  },
  "Root": {
    "props": {
      "align": {
        "type": "'center' | 'end' | 'start'",
        "defaultValue": "\"start\"",
        "isRequired": false,
        "description": "The alignment of the slides in the carousel."
      },
      "asChild": {
        "type": "boolean",
        "isRequired": false,
        "description": "Render as a different element type."
      },
      "defaultIndex": {
        "type": "number",
        "isRequired": false,
        "description": "The initial index of the carousel when it is first rendered.\nUse this when you do not need to control the state of the carousel."
      },
      "id": {
        "type": "string",
        "isRequired": false,
        "description": "The unique identifier of the machine."
      },
      "ids": {
        "type": "Partial<{\n  root: string\n  viewport: string\n  slide(index: number): string\n  slideGroup: string\n  nextSlideTrigger: string\n  prevSlideTrigger: string\n  indicatorGroup: string\n  indicator(index: number): string\n}>",
        "isRequired": false,
        "description": "The ids of the elements in the carousel. Useful for composition."
      },
      "index": { "type": "number", "isRequired": false, "description": "The current slide index." },
      "loop": {
        "type": "boolean",
        "defaultValue": "false",
        "isRequired": false,
        "description": "Whether the carousel should loop around."
      },
      "onIndexChange": {
        "type": "(details: SlideChangeDetails) => void",
        "isRequired": false,
        "description": "Function called when the slide changes."
      },
      "orientation": {
        "type": "'horizontal' | 'vertical'",
        "defaultValue": "\"horizontal\"",
        "isRequired": false,
        "description": "The orientation of the carousel."
      },
      "slidesPerView": {
        "type": "number | 'auto'",
        "defaultValue": "1",
        "isRequired": false,
        "description": "The number of slides to show at a time."
      },
      "spacing": {
        "type": "string",
        "defaultValue": "\"0px\"",
        "isRequired": false,
        "description": "The amount of space between slides."
      }
    },
    "element": "HTMLDivElement"
  },
  "RootProvider": {
    "props": {
      "value": { "type": "UseCarouselReturn", "isRequired": true },
      "asChild": {
        "type": "boolean",
        "isRequired": false,
        "description": "Render as a different element type."
      }
    },
    "element": "HTMLDivElement"
  },
  "Viewport": {
    "props": {
      "asChild": {
        "type": "boolean",
        "isRequired": false,
        "description": "Render as a different element type."
      }
    },
    "element": "HTMLDivElement"
  }
}
