{
  "Basic": "<script setup lang=\"ts\">\nimport {\n  Carousel,\n  CarouselControl,\n  CarouselIndicator,\n  CarouselIndicatorGroup,\n  CarouselNextSlideTrigger,\n  CarouselPrevSlideTrigger,\n  CarouselSlide,\n  CarouselSlideGroup,\n  CarouselViewport,\n} from '@ark-ui/vue'\n\nconst images = [\n  'https://tinyurl.com/5b6ka8jd',\n  'https://tinyurl.com/7rmccdn5',\n  'https://tinyurl.com/59jxz9uu',\n  'https://tinyurl.com/6jurv23t',\n  'https://tinyurl.com/yp4rfum7',\n]\n</script>\n\n<template>\n  <Carousel>\n    <CarouselControl>\n      <CarouselPrevSlideTrigger>Prev</CarouselPrevSlideTrigger>\n      <CarouselNextSlideTrigger>Next</CarouselNextSlideTrigger>\n    </CarouselControl>\n    <CarouselViewport>\n      <CarouselSlideGroup>\n        <CarouselSlide v-for=\"(image, idx) in images\" :key=\"idx\" :index=\"idx\">\n          <img\n            :src=\"image\"\n            alt=\"\"\n            :style=\"{ height: '300px', width: '100%', objectFit: 'cover' }\"\n          />\n        </CarouselSlide>\n      </CarouselSlideGroup>\n    </CarouselViewport>\n    <CarouselIndicatorGroup>\n      <CarouselIndicator v-for=\"(_, idx) in images\" :key=\"idx\" :index=\"idx\">\n        {{ idx + 1 }}\n      </CarouselIndicator>\n    </CarouselIndicatorGroup>\n  </Carousel>\n</template>\n"
}
