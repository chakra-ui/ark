{
  "name": "@ark-ui/mcp",
  "description": "The official MCP server for Ark UI",
  "version": "1.0.0",
  "main": "dist/stdio.js",
  "scripts": {
    "build": "tsup && chmod 755 dist/stdio.js",
    "dev": "tsup --watch",
    "serve": "tsx --watch src/http.ts",
    "inspect": "bun scripts/inspect.js"
  },
  "bin": {
    "ark-mcp": "./dist/stdio.js"
  },
  "keywords": [
    "ark-ui",
    "mcp-server",
    "chat",
    "chatgpt",
    "mcp"
  ],
  "type": "module",
  "license": "MIT",
  "devDependencies": {
    "@tsconfig/node22": "^22.0.2",
    "@types/express": "^5.0.3",
    "tsup": "^8.5.0",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.17.0",
    "express": "^5.1.0",
    "zod": "^3.24.4"
  }
}
